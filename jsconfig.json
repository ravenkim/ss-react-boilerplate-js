{
    "compilerOptions": {
        "jsx": "react-jsx",
        "target": "ES6",
        "module": "commonjs",
        "baseUrl": ".",
        "paths": {
            "src/*": ["src/*"]
        },
        "allowJs": true,
        "checkJs": false,
        "types": ["node"]
    },
    "include": ["src/**/*"],
    "exclude": ["dist", "node_modules", ".yarn"]
}
